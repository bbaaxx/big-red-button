{
  "name": "big-red-button",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ubuild": "rimraf ./dist && NODE_ENV=build webpack --config ./config/webpack/webpack.config.development.js",
    "udev": "webpack-dashboard -- webpack-dev-server --config ./config/webpack/webpack.config.development.js",
    "ustart": "yarn run build && http-server ./dist/web",
    "utest": "NODE_ENV=test jest --config ./.jestconfig.json",
    "udevop": "nodemon ./dot/server.js --watch config --watch dot",
    "clean:build": "rimraf build",
    "prebuild": "yarn run clean:build",
    "build": "yarn run build:client && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.build.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.build.js --progress",
    "build:profile": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.build.js --profile --json > profile.json",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "prestart": "yarn run clean:build",
    "start": "webpack-dashboard -- node ./dot/server.js",
    "test": "cross-env BABEL_ENV=commonjs jest --no-cache --config ./.jestconfig.json",
    "test:cov": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint config src/** --ext .js",
    "tunnel": "lt --port 3000"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "hammerjs": "^2.0.8",
    "http-server": "^14.1.1",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "nodemon": "^3.1.7",
    "roboto-fontface": "^0.10.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-classes": "^7.25.9",
    "@babel/plugin-transform-destructuring": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-flow": "^7.25.9",
    "ajv": "^8.17.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "buble-loader": "^0.5.1",
    "caniuse-lite": "^1.0.30001680",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "exports-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "flow": "^0.2.3",
    "flow-babel-webpack-plugin": "^1.1.1",
    "flow-bin": "^0.248.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lost": "^9.0.2",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.1",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "script-loader": "^0.7.2",
    "style-loader": "^4.0.0",
    "stylelint": "^16.11.0",
    "url-loader": "^4.1.1",
    "wallaby-webpack": "^3.2.19",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-dev-server": "^5.1.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^6.0.1"
  }
}
